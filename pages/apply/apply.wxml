<view class="line">
<form bindsubmit="formSubmit" bindreset="formReset">
  <wux-cell hover-class="none">
    <input name="yys_name" placeholder="请输入游戏昵称">游戏昵称</input>
  </wux-cell>
  <wux-cell hover-class="none">
    <input name="yys_id" placeholder="请输入游戏id（点击游戏头像-编号）" type="number">游戏id</input>
  </wux-cell>

  <button form-type="submit">提交</button>
  </form>
</view>