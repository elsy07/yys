.me {
    padding: 30rpx;
    position:fixed; 
    bottom:0;
    right: 30rpx;
}

.me image {
    width: 150rpx;
    height: 150rpx;
}


.container {
    width: 100vw;
    height: 100vh;
}


.times-wrap {
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.6);
    z-index: 9;
}

.times-wrap image {
    width: 580rpx;
    height: 580rpx;
}

.times-wrap text {
    margin-top: 50rpx;
    font-size: 38rpx;
    color: #fff;
}

.no-permission-wrap {
    position: absolute;
    top: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
    height: calc(100vh - 32rpx);
    padding-top: 240rpx;
    background-color: rgba(255,255,255,.9);
    font-size: 32rpx;
    color: #333;
    z-index: 9;
}

.no-permission-wrap image {
    width: 120rpx;
    height: 120rpx;
    margin-bottom: 10rpx;
}

.no-permission-wrap text {
    margin-top: 30rpx;
    line-height: 1;
}

.no-permission-wrap .color-font {
    color: #34A853;
    font-weight: 600;
}

.no-permission-wrap .btn {
    width: 200rpx;
    height: 80rpx;
    margin-top: 40rpx;
    text-align: center;
    line-height: 80rpx;
    background-color: #34A853;
    color: #fff;
    font-size: 30rpx;
    border-radius: 10rpx;
}

button {
    background: none;
    border-radius: 0;
    text-align: left;
}

button::after {
    border: none;
}